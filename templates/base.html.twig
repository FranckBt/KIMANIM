<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>Kimanim{% block title %}{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header>
            <div>
                <h1><a href="{{path('app_homepage')}}">Kimanim</a></h1>
            </div>
            <nav>
                <ul class = "flex">
                    <li><a href="{{path('app_homepage')}}">Acceuil</a></li>
                    <li><a href="#">Inscription</a></li>
                    <li><a href="{{path('login')}}">Se connecter</a></li>
                    <li><a href="#">Activités</a></li>
                </ul>
            </nav>
        </header>
        {% block body %}{% endblock %}
        <footer>
            <div class="flex">
                <ul>
                    <li><a href="#">Vous êtes animateurs</a></li>
                    <li><a href="#">Vous êtes parents</a></li>
                    <li><a href="{{path('app_faq')}}">FAQ</a></li>
                </ul>
                <ul>
                    <li><a href="#">Facebook</a></li>
                    <li><a href="#">Twitter</a></li>
                    <li><a href="#">Linkedin</a></li>
                </ul>
                <ul>
                    <li><a href="{{path('app_contact')}}">Contactez-nous</a></li>
                    <li><a href="{{path('app_mentions')}}">Mentions légales</a></li>
                </ul>
            </div>
            <p>&copy Kimanim 2022</p>
        </footer>
    </body>
</html>
